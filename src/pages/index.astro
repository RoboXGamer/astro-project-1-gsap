---
// Layout
import Layout from "../layouts/Layout.astro";
// Pages
import Page1 from "../components/pages/Page1.astro";
import Page2 from "../components/pages/Page2.astro";
import Page3 from "../components/pages/Page3.astro";
---

<Layout title="Welcome to Astro.">
	<main>
		<Page1 />
		<Page2 />
		<Page3 />
	</main>
</Layout>

<style>
	main {
		background-color: rgb(var(--primary));
		min-height: 100vh;

		position: relative;
		overflow: hidden;
	}
</style>
